  "devDependencies": {
    "@types/graphql-upload": "^17.0.0",
    "@types/jest": "29.2.4",
    "@types/node": "22.10.6",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "tsc-watch": "5.0.3",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
